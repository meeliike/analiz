<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Paketli Gıda Analiz Sistemi</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.0/font/bootstrap-icons.css">
    <style>
        :root {
            --color-primary: #AED8CC;
            --color-accent: #CD6688;
            --color-dark: #7A016F;
            --color-darkest: #461959;
        }

        body {
            background: linear-gradient(135deg, #AED8CC 0%, #f0f8f5 100%);
            color: var(--color-darkest);
            min-height: 100vh;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }

        .navbar {
            background-color: var(--color-darkest) !important;
            padding: 0.75rem 0;
            box-shadow: 0 2px 4px rgba(70, 25, 89, 0.2);
        }

        .navbar-brand {
            color: var(--color-primary) !important;
            font-weight: bold;
            font-size: 1.3rem;
        }

        .hamburger-menu {
            background: none;
            border: none;
            color: var(--color-primary);
            font-size: 1.5rem;
            padding: 0.5rem;
            cursor: pointer;
            transition: all 0.3s;
        }

        .hamburger-menu:hover {
            color: white;
            transform: scale(1.1);
        }

        .side-panel {
            position: fixed;
            left: -350px;
            top: 0;
            width: 350px;
            height: 100vh;
            background-color: var(--color-darkest);
            color: var(--color-primary);
            z-index: 1000;
            transition: left 0.3s ease;
            box-shadow: 2px 0 10px rgba(0,0,0,0.3);
            padding: 2rem;
            overflow-y: auto;
        }

        .side-panel.active {
            left: 0;
        }

        .side-panel-overlay {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 999;
            display: none;
        }

        .side-panel-overlay.active {
            display: block;
        }

        .side-panel h3 {
            color: var(--color-primary);
            margin-bottom: 1.5rem;
            border-bottom: 2px solid var(--color-primary);
            padding-bottom: 1rem;
        }

        .side-panel-close {
            position: absolute;
            top: 1rem;
            right: 1rem;
            background: none;
            border: none;
            color: var(--color-primary);
            font-size: 1.5rem;
            cursor: pointer;
        }

        .search-bar {
            cursor: pointer;
            background-color: white;
            border: 2px solid var(--color-dark);
            border-radius: 25px;
            padding: 0.75rem 1.5rem;
            width: 100%;
            max-width: 600px;
            margin: 1rem auto;
            display: flex;
            align-items: center;
            justify-content: space-between;
            box-shadow: 0 2px 4px rgba(70, 25, 89, 0.1);
        }

        .search-input-container {
            display: none;
            flex: 1;
            margin-right: 1rem;
        }

        .search-input-container.active {
            display: block;
        }

        .search-input-container input {
            border: none;
            outline: none;
            width: 100%;
            padding: 0.5rem;
            font-size: 1rem;
        }

        .search-button {
            background: none;
            border: none;
            color: var(--color-darkest);
            font-size: 1.2rem;
            cursor: pointer;
            padding: 0.5rem;
            transition: all 0.3s;
        }

        .search-button:hover {
            color: var(--color-accent);
        }

        .camera-button {
            background-color: var(--color-accent);
            border: none;
            color: white;
            padding: 0.5rem 1rem;
            border-radius: 20px;
            cursor: pointer;
            margin-left: 0.5rem;
            transition: all 0.3s;
        }

        .camera-button:hover {
            background-color: var(--color-dark);
        }

        .camera-container {
            display: none;
            margin-top: 1rem;
            text-align: center;
        }

        .camera-container.active {
            display: block;
        }

        #video {
            width: 100%;
            max-width: 400px;
            border-radius: 15px;
            border: 3px solid var(--color-dark);
            background-color: #000;
        }

        .main-container {
            padding: 1rem 0;
        }

        .card {
            background-color: white;
            border: 2px solid var(--color-dark);
            border-radius: 15px;
            box-shadow: 0 4px 6px rgba(70, 25, 89, 0.1);
            margin-bottom: 1.5rem;
            overflow: hidden;
            height: 100%;
        }

        #categoriesList {
            max-height: 400px;
            overflow-y: auto;
        }

        .card-header {
            background: linear-gradient(135deg, var(--color-dark) 0%, var(--color-accent) 100%);
            color: white;
            padding: 1rem 1.5rem;
            border-bottom: none;
        }

        .card-header h5 {
            margin: 0;
            font-weight: bold;
        }

        .category-item {
            padding: 1rem;
            border-bottom: 1px solid #e9ecef;
            cursor: pointer;
            transition: all 0.2s;
        }

        .category-item:hover {
            background-color: var(--color-primary);
        }

        .category-item.active {
            background-color: var(--color-accent);
            color: white;
        }

        .product-item {
            padding: 1rem;
            border-bottom: 1px solid #e9ecef;
            cursor: pointer;
            transition: all 0.2s;
            display: flex;
            align-items: center;
        }

        .product-item:hover {
            background-color: var(--color-primary);
        }

        .product-item img {
            width: 60px;
            height: 60px;
            object-fit: cover;
            border-radius: 8px;
            margin-right: 1rem;
            border: 2px solid var(--color-dark);
        }

        .product-item-content {
            flex: 1;
        }

        .news-card {
            padding: 1.5rem;
        }

        .news-title {
            font-size: 1.2rem;
            font-weight: bold;
            color: var(--color-darkest);
            margin-bottom: 0.5rem;
        }

        .news-content {
            color: #6c757d;
            margin-bottom: 1rem;
            line-height: 1.6;
        }

        .news-date {
            font-size: 0.85rem;
            color: var(--color-accent);
        }

        .btn-refresh {
            background-color: var(--color-accent);
            border: none;
            color: white;
            padding: 0.5rem 1.5rem;
            border-radius: 20px;
            cursor: pointer;
            transition: all 0.3s;
            width: 100%;
            margin-top: 1rem;
        }

        .btn-refresh:hover {
            background-color: var(--color-dark);
            transform: translateY(-2px);
        }

        .product-detail-card {
            display: none;
            margin-top: 2rem;
        }

        .product-detail-card.active {
            display: block;
        }

        .score-circle {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 2rem;
            font-weight: bold;
            margin: 0 auto;
            border: 6px solid;
            color: var(--color-darkest);
        }

        .score-high {
            background-color: #d4edda;
            border-color: #28a745;
            color: #155724;
        }

        .score-medium {
            background-color: #fff3cd;
            border-color: #ffc107;
            color: #856404;
        }

        .score-low {
            background-color: #f8d7da;
            border-color: #dc3545;
            color: #721c24;
        }

        .risk-badge {
            background-color: var(--color-accent);
            color: white;
            padding: 0.25rem 0.75rem;
            border-radius: 15px;
            font-size: 0.85rem;
            font-weight: bold;
            cursor: pointer;
            display: inline-block;
            margin: 0.25rem;
        }

        .risk-badge:hover {
            background-color: var(--color-dark);
        }

        .ingredient-item {
            padding: 0.5rem;
            margin: 0.25rem 0;
            border-radius: 8px;
            background-color: #f8f9fa;
        }

        .risk-detail {
            background-color: #fff3f8;
            border-left: 4px solid var(--color-accent);
            padding: 0.75rem;
            margin-top: 0.5rem;
            border-radius: 8px;
            display: none;
        }

        .risk-detail.show {
            display: block;
        }

        footer {
            background-color: var(--color-darkest);
            color: var(--color-primary);
            padding: 1.5rem 0;
            margin-top: 2rem;
            text-align: center;
        }

        .loading {
            text-align: center;
            padding: 2rem;
            display: none;
        }

        .loading.active {
            display: block;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark">
        <div class="container-fluid">
            <button class="hamburger-menu" id="hamburgerMenu" title="Gelecekte eklenecek ikinci kısım">
                <i class="bi bi-list"></i>
            </button>
            <a class="navbar-brand" href="#">
                <i class="bi bi-shield-check"></i> Paketli Gıda Analiz Sistemi
            </a>
        </div>
    </nav>

    <!-- Sol Panel -->
    <div class="side-panel-overlay" id="sidePanelOverlay"></div>
    <div class="side-panel" id="sidePanel">
        <button class="side-panel-close" id="sidePanelClose">
            <i class="bi bi-x-lg"></i>
        </button>
        <h3>İkinci Özellik</h3>
        <p>Bu bölüm gelecekte eklenecek ikinci özellik için hazırlanmıştır.</p>
    </div>

    <!-- Barkod Arama Çubuğu -->
    <div class="container-fluid">
        <div class="search-bar">
            <div class="search-input-container" id="searchInputContainer">
                <input type="text" id="barcodeInput" placeholder="Barkod numarası veya ürün adı girin..." onkeypress="handleBarcodeEnter(event)">
            </div>
            <button class="search-button" id="searchButton" title="Ara">
                <i class="bi bi-search"></i>
            </button>
            <button class="camera-button" id="cameraButton" title="Kamera ile Tara">
                <i class="bi bi-camera"></i>
            </button>
        </div>
        <div class="camera-container" id="cameraContainer">
            <video id="video" autoplay playsinline></video>
            <div class="mt-2">
                <button class="btn btn-sm btn-primary" id="captureBtn">Yakala</button>
                <button class="btn btn-sm btn-secondary" id="stopBtn">Durdur</button>
            </div>
        </div>
    </div>

    <!-- Ana İçerik - İki Kolon -->
    <div class="container-fluid main-container">
        <div class="row">
            <!-- Sol Kolon: Kategoriler ve Ürünler -->
            <div class="col-lg-6">
                <div class="row">
                    <div class="col-md-5">
                        <div class="card">
                            <div class="card-header">
                                <h5><i class="bi bi-grid"></i> Kategoriler</h5>
                            </div>
                            <div class="card-body p-0">
                                <div class="loading" id="categoriesLoading">
                                    <div class="spinner-border text-primary" role="status"></div>
                                </div>
                                <div id="categoriesList"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-7">
                        <div class="card">
                            <div class="card-header">
                                <h5><i class="bi bi-box"></i> Ürünler</h5>
                            </div>
                            <div class="card-body p-0">
                                <div id="productsList">
                                    <div class="p-3 text-center text-muted">
                                        Kategori seçiniz
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Sağ Kolon: Haberler -->
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h5><i class="bi bi-newspaper"></i> Gıda Sektörü Haberleri</h5>
                    </div>
                    <div class="card-body">
                        <div class="loading" id="newsLoading">
                            <div class="spinner-border text-primary" role="status"></div>
                        </div>
                        <div class="news-card" id="newsCard">
                            <div class="news-title" id="newsTitle"></div>
                            <div class="news-content" id="newsContent"></div>
                            <div class="news-date" id="newsDate"></div>
                        </div>
                        <button class="btn-refresh" id="refreshNews">
                            <i class="bi bi-arrow-clockwise"></i> Yeni Haber
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Ürün Detay Kartı -->
    <div class="container-fluid">
        <div class="card product-detail-card" id="productDetailCard">
            <div class="card-header">
                <h4 class="mb-0" id="productName"></h4>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-4 text-center">
                        <img id="productImage" src="" alt="Ürün" class="img-fluid mb-3" style="max-height: 300px; border-radius: 15px;">
                        <div class="category-badge" id="productCategory" style="background-color: var(--color-primary); color: var(--color-darkest); padding: 0.5rem 1rem; border-radius: 20px;"></div>
                    </div>
                    <div class="col-md-8">
                        <div class="text-center mb-4">
                            <div class="score-circle" id="scoreCircle">
                                <span id="scoreValue"></span>
                            </div>
                            <p class="mt-2"><strong>Sağlık Skoru</strong></p>
                        </div>
                        <h5><i class="bi bi-list-ul"></i> İçindekiler</h5>
                        <div id="ingredientsList"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="container">
            <p class="mb-0">&copy; 2025 Paketli Gıda Analiz Sistemi</p>
        </div>
    </footer>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="barcode_scanner.js"></script>
    <script>
        let stream = null;
        let selectedCategoryId = null;

        // Sayfa yüklendiğinde
        document.addEventListener('DOMContentLoaded', () => {
            loadCategories();
            loadNews();
        });

        // Hamburger menü
        const hamburgerMenu = document.getElementById('hamburgerMenu');
        const sidePanel = document.getElementById('sidePanel');
        const sidePanelOverlay = document.getElementById('sidePanelOverlay');
        const sidePanelClose = document.getElementById('sidePanelClose');

        hamburgerMenu.addEventListener('click', () => {
            sidePanel.classList.add('active');
            sidePanelOverlay.classList.add('active');
        });

        sidePanelClose.addEventListener('click', () => {
            sidePanel.classList.remove('active');
            sidePanelOverlay.classList.remove('active');
        });

        sidePanelOverlay.addEventListener('click', () => {
            sidePanel.classList.remove('active');
            sidePanelOverlay.classList.remove('active');
        });

        // Arama çubuğuna tıklayınca input açılsın
        const searchBar = document.querySelector('.search-bar');
        const searchButton = document.getElementById('searchButton');
        const searchInputContainer = document.getElementById('searchInputContainer');

        searchBar.addEventListener('click', (e) => {
            // Butonlara tıklanırsa özel işlemler yapılsın
            if (e.target.closest('.search-button') || e.target.closest('.camera-button')) {
                return;
            }
            searchInputContainer.classList.add('active');
            document.getElementById('barcodeInput').focus();
        });

        // Arama butonu
        searchButton.addEventListener('click', (e) => {
            e.stopPropagation();
            searchInputContainer.classList.toggle('active');
            if (searchInputContainer.classList.contains('active')) {
                document.getElementById('barcodeInput').focus();
            }
        });

        // Barkod Enter tuşu
        function handleBarcodeEnter(event) {
            if (event.key === 'Enter') {
                const barcode = document.getElementById('barcodeInput').value.trim();
                if (barcode) {
                    scanProduct(barcode);
                }
            }
        }

        // Kamera butonu
        document.getElementById('cameraButton').addEventListener('click', async () => {
            const videoElement = document.getElementById('video');
            const cameraContainer = document.getElementById('cameraContainer');
            
            try {
                cameraContainer.classList.add('active');
                
                // ZXing ile barkod taramayı başlat
                await window.barcodeScanner.startScanning(
                    videoElement,
                    // Başarı callback
                    (barcode) => {
                        scanProduct(barcode);
                        stopCamera();
                    },
                    // Hata callback
                    (error) => {
                        alert('Kamera hatası: ' + error.message);
                        stopCamera();
                    }
                );
            } catch (error) {
                alert('Kamera başlatılamadı: ' + error.message);
                stopCamera();
            }
        });

        // Yakala butonu - direkt input açılsın
        document.getElementById('captureBtn').addEventListener('click', () => {
            document.getElementById('searchInputContainer').classList.add('active');
            const input = document.getElementById('barcodeInput');
            input.focus();
            input.select();
            stopCamera();
        });

        // Durdur butonu
        document.getElementById('stopBtn').addEventListener('click', stopCamera);

        function stopCamera() {
            // ZXing taramayı durdur
            if (window.barcodeScanner) {
                window.barcodeScanner.stopScanning();
            }
            
            // Eski stream'i temizle (geriye dönük uyumluluk)
            if (stream) {
                stream.getTracks().forEach(track => track.stop());
                stream = null;
            }
            
            document.getElementById('cameraContainer').classList.remove('active');
        }

        // Kategorileri yükle
        async function loadCategories() {
            const loading = document.getElementById('categoriesLoading');
            const container = document.getElementById('categoriesList');
            
            loading.classList.add('active');
            
            try {
                const response = await fetch('categories.php');
                const data = await response.json();
                
                console.log('Kategori yanıtı:', data); // Debug
                
                if (data.basari && data.kategoriler) {
                    displayCategories(data.kategoriler);
                } else {
                    container.innerHTML = `<div class="p-3 text-danger">Kategori yüklenemedi: ${data.hata || 'Bilinmeyen hata'}</div>`;
                    console.error('Kategori hatası:', data);
                }
            } catch (error) {
                container.innerHTML = `<div class="p-3 text-danger">Kategoriler yüklenemedi: ${error.message}</div>`;
                console.error('Fetch hatası:', error);
            } finally {
                loading.classList.remove('active');
            }
        }

        // Kategorileri göster
        function displayCategories(kategoriler) {
            const container = document.getElementById('categoriesList');
            container.innerHTML = '';
            
            if (kategoriler.length === 0) {
                container.innerHTML = '<div class="p-3 text-muted text-center">Kategori bulunamadı</div>';
                return;
            }
            
            kategoriler.forEach(kategori => {
                const item = document.createElement('div');
                item.className = 'category-item';
                item.innerHTML = `
                    <strong>${escapeHtml(kategori.isim)}</strong>
                    <span class="badge bg-secondary float-end">${kategori.urun_sayisi}</span>
                `;
                item.addEventListener('click', () => {
                    // Aktif kategoriyi işaretle
                    document.querySelectorAll('.category-item').forEach(el => el.classList.remove('active'));
                    item.classList.add('active');
                    selectedCategoryId = kategori.id;
                    loadProducts(kategori.id);
                });
                container.appendChild(item);
            });
        }

        // Ürünleri yükle
        async function loadProducts(categoryId) {
            const container = document.getElementById('productsList');
            container.innerHTML = '<div class="p-3 text-center"><div class="spinner-border spinner-border-sm"></div></div>';
            
            try {
                const response = await fetch(`products.php?category_id=${categoryId}`);
                const data = await response.json();
                
                if (data.basari && data.urunler) {
                    displayProducts(data.urunler);
                } else {
                    container.innerHTML = '<div class="p-3 text-muted text-center">Ürün bulunamadı</div>';
                }
            } catch (error) {
                container.innerHTML = '<div class="p-3 text-danger">Ürünler yüklenemedi</div>';
            }
        }

        // Ürünleri göster
        function displayProducts(urunler) {
            const container = document.getElementById('productsList');
            container.innerHTML = '';
            
            if (urunler.length === 0) {
                container.innerHTML = '<div class="p-3 text-muted text-center">Bu kategoride ürün yok</div>';
                return;
            }
            
            urunler.forEach(urun => {
                const item = document.createElement('div');
                item.className = 'product-item';
                const imageUrl = urun.gorsel_url || 'https://via.placeholder.com/60x60?text=Ürün';
                item.innerHTML = `
                    <img src="${escapeHtml(imageUrl)}" alt="${escapeHtml(urun.isim)}">
                    <div class="product-item-content">
                        <strong>${escapeHtml(urun.isim)}</strong>
                        <div class="text-muted small">${escapeHtml(urun.kategori_adi || '')}</div>
                    </div>
                `;
                item.addEventListener('click', () => {
                    scanProduct(urun.barkod);
                });
                container.appendChild(item);
            });
        }

        // Haberleri yükle
        async function loadNews() {
            const loading = document.getElementById('newsLoading');
            loading.classList.add('active');
            
            try {
                const response = await fetch('news.php');
                const data = await response.json();
                
                if (data.basari && data.haber) {
                    displayNews(data.haber);
                }
            } catch (error) {
                console.error('Haber yüklenemedi:', error);
            } finally {
                loading.classList.remove('active');
            }
        }

        // Haberi göster
        function displayNews(haber) {
            document.getElementById('newsTitle').textContent = haber.baslik;
            document.getElementById('newsContent').textContent = haber.icerik;
            document.getElementById('newsDate').textContent = 'Tarih: ' + haber.tarih;
        }

        // Haber yenile butonu
        document.getElementById('refreshNews').addEventListener('click', loadNews);

        // Ürün tarama (barkod veya isim ile)
        async function scanProduct(searchTerm) {
            document.getElementById('productDetailCard').classList.remove('active');
            
            if (!searchTerm || searchTerm.trim() === '') {
                return;
            }
            
            try {
                const response = await fetch(`product.php?barcode=${encodeURIComponent(searchTerm.trim())}`);
                const data = await response.json();
                
                if (data.hata) {
                    alert('Hata: ' + data.hata);
                    return;
                }
                
                displayProductDetail(data);
                document.getElementById('barcodeInput').value = '';
            } catch (error) {
                alert('API hatası: ' + error.message);
            }
        }

        // Ürün detayını göster
        function displayProductDetail(data) {
            document.getElementById('productName').textContent = data.urun_adi;
            document.getElementById('productCategory').textContent = data.kategori || 'Kategori yok';
            
            const img = document.getElementById('productImage');
            if (data.gorsel_url && data.gorsel_url.trim() !== '') {
                img.src = data.gorsel_url;
            } else {
                img.src = 'https://via.placeholder.com/300x300?text=Görsel+Yok';
            }
            
            // Skor
            const scoreCircle = document.getElementById('scoreCircle');
            const scoreValue = document.getElementById('scoreValue');
            scoreValue.textContent = data.genel_skor;
            
            scoreCircle.classList.remove('score-high', 'score-medium', 'score-low');
            if (data.genel_skor >= 70) {
                scoreCircle.classList.add('score-high');
            } else if (data.genel_skor >= 40) {
                scoreCircle.classList.add('score-medium');
            } else {
                scoreCircle.classList.add('score-low');
            }
            
            // İçindekiler
            const ingredientsList = document.getElementById('ingredientsList');
            ingredientsList.innerHTML = '';
            
            if (!data.icerikler || data.icerikler.length === 0) {
                ingredientsList.innerHTML = '<p class="text-muted">İçerik bilgisi yok</p>';
                return;
            }
            
            const riskliIsimler = data.riskli_icerikler ? data.riskli_icerikler.map(r => r.isim) : [];
            
            data.icerikler.forEach(ingredient => {
                const item = document.createElement('div');
                item.className = 'ingredient-item';
                
                const isRiskli = riskliIsimler.some(riskli => 
                    ingredient.toLowerCase().includes(riskli.toLowerCase()) || 
                    riskli.toLowerCase().includes(ingredient.toLowerCase())
                );
                
                if (isRiskli) {
                    const badge = document.createElement('span');
                    badge.className = 'risk-badge';
                    badge.textContent = ingredient;
                    badge.addEventListener('click', () => toggleRiskDetail(ingredient, item, data.riskli_icerikler));
                    item.appendChild(badge);
                } else {
                    item.textContent = ingredient;
                }
                
                const detail = document.createElement('div');
                detail.className = 'risk-detail';
                detail.id = `risk-${ingredient.replace(/\s+/g, '-')}`;
                item.appendChild(detail);
                
                ingredientsList.appendChild(item);
            });
            
            document.getElementById('productDetailCard').classList.add('active');
            document.getElementById('productDetailCard').scrollIntoView({ behavior: 'smooth' });
        }

        // Risk detayı
        function toggleRiskDetail(ingredient, parent, riskliIcerikler) {
            const detail = document.getElementById(`risk-${ingredient.replace(/\s+/g, '-')}`);
            
            document.querySelectorAll('.risk-detail.show').forEach(d => {
                if (d.id !== detail.id) d.classList.remove('show');
            });
            
            if (detail.classList.contains('show')) {
                detail.classList.remove('show');
            } else {
                const riskInfo = riskliIcerikler.find(r => 
                    ingredient.toLowerCase().includes(r.isim.toLowerCase()) || 
                    r.isim.toLowerCase().includes(ingredient.toLowerCase())
                );
                
                if (riskInfo) {
                    detail.innerHTML = `<strong>${escapeHtml(riskInfo.isim)}</strong><br>${escapeHtml(riskInfo.zarar_nedeni)}`;
                }
                detail.classList.add('show');
            }
        }

        // XSS koruması
        function escapeHtml(text) {
            const div = document.createElement('div');
            div.textContent = text;
            return div.innerHTML;
        }
    </script>
</body>
</html>
